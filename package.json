{
  "name": "homework-viewer",
  "version": "1.2.0",
  "description": "View and add homework !",
  "main": "app.js",
  "dependencies": {
    "electron-context-menu": "^0.9.1",
    "electron-is-dev": "^2.0.0",
    "jquery": "^3.4.1"
  },
  "devDependencies": {
    "electron": "^12.0.2",
    "electron-packager": "^14.0.3"
  },
  "scripts": {
    "start": "electron .",
    "build-linux": "electron-packager ./ --overwrite --asar --platform=linux --arch=x64 --icon=./assets/icons/main.ico --version-string.LegalCopyright=Mrthomas20121 --version-string.OriginalFilename=HomeworkViewer.exe",
    "build": "electron-packager ./ --asar --save=array-unique --version-string.LegalCopyright=Mrthomas20121 --version-string.OriginalFilename=HomeworkViewer.exe",
    "build-mac": "electron-packager ./ --asar --overwrite --platform=darwin --arch=x64 --icon=./assets/icons/main.ico --version-string.LegalCopyright=Mrthomas20121 --version-string.OriginalFilename=HomeworkViewer.exe"
  },
